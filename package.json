{
  "name": "hemera-mongo-store",
  "version": "5.0.0",
  "description": "This is a plugin to use Mongodb with Hemera",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:hemerajs/hemera-mongo-store.git",
    "type": "git"
  },
  "scripts": {
    "lint": "eslint ./**/*.js",
    "test": "nyc mocha -t 5000 --exit ./test/**/*.js",
    "coverage": "npm run test && nyc report --reporter=html",
    "patch": "np patch --yolo",
    "minor": "np minor --yolo",
    "major": "np major --yolo"
  },
  "keywords": [
    "mongodb",
    "mongo",
    "hemera"
  ],
  "engines": {
    "node": ">=6"
  },
  "author": "Dustin Deus (https://github.com/StarpTech)",
  "license": "MIT",
  "dependencies": {
    "hemera-joi": "5.0.x",
    "hemera-plugin": "1.1.x",
    "hemera-store": "^1.1.x",
    "mongodb": "2.2.x",
    "mongodb-extended-json": "1.10.x"
  },
  "devDependencies": {
    "code": "^3.0.2",
    "eslint": "^4.19.0",
    "eslint-config-hemera": "^0.0.2",
    "hemera-testsuite": "^4.0.0",
    "mocha": "^5.0.4",
    "nats": "^0.8.4",
    "nats-hemera": "^5.0.0-rc.1",
    "np": "^2.20.1",
    "nyc": "^11.6.0",
    "prettier": "^1.11.1"
  }
}
